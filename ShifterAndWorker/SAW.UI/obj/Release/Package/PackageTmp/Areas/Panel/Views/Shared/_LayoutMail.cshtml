
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta name="description" content="admin-themes-lab">
    <meta name="author" content="themes-lab">
    <meta name="robots" content="noindex" />
    <link rel="shortcut icon" href="~/Areas/Panel/assets/global/images/favicon.png" type="image/png">
    <title>@ViewBag.Title - Shifter and Worker</title>
    <link href='http://fonts.googleapis.com/css?family=Merriweather' rel='stylesheet' type='text/css'>
    <link href="~/Areas/Panel/assets/global/css/style.css" rel="stylesheet">
    <link href="~/Areas/Panel/assets/global/css/theme.css" rel="stylesheet">
    <link href="~/Areas/Panel/assets/global/css/ui.css" rel="stylesheet">
    <link href="~/Areas/Panel/assets/admin/layout/css/layout.css" rel="stylesheet">
    <script src="~/Areas/Panel/assets/global/plugins/modernizr/modernizr-2.6.2-respond-1.1.0.min.js"></script>
    <!-- BEGIN PAGE STYLE -->
    <link href="~/Areas/Panel/assets/global/plugins/charts-morris/morris.min.css" rel="stylesheet">
    <!-- END PAGE STYLE -->
</head>
<body class="fixed-topbar fixed-sidebar theme-sdtl color-default mailbox">
    <!--[if lt IE 7]>
    <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <section>

        @Html.Action("GetSideBarMailPartial", "Mail")

        <div class="main-content">
            @Html.Action("GetTopBarPartial", "Home")

            @RenderBody()

            <!-- BEGIN FOOTER -->
            <div class="footer">
                <div class="copyright">
                    <p class="pull-left sm-pull-reset">
                        Copyright &copy; @DateTime.Now.Year Shifter and Worker | All rights reserved.
                    </p>
                    <p class="pull-right sm-pull-reset">
                        <span><a href="#" class="m-r-10">Support</a> | <a href="#" class="m-l-10 m-r-10">Terms of use</a> | <a href="#" class="m-l-10">Privacy Policy</a></span>
                    </p>
                </div>
            </div>
            <!-- END FOOTER -->
        </div>

    </section>

    <!-- BEGIN PRELOADER -->
    <div class="loader-overlay">
        <div class="spinner">
            <div class="bounce1"></div>
            <div class="bounce2"></div>
            <div class="bounce3"></div>
        </div>
    </div>
    <!-- END PRELOADER -->

    <a href="#" class="scrollup"><i class="fa fa-angle-up"></i></a>
    <script src="~/Areas/Panel/assets/global/plugins/jquery/jquery-3.1.0.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/jquery/jquery-migrate-3.0.0.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/gsap/main-gsap.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/tether/js/tether.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/appear/jquery.appear.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/jquery-cookies/jquery.cookies.min.js"></script> <!-- Jquery Cookies, for theme -->
    <script src="~/Areas/Panel/assets/global/plugins/jquery-block-ui/jquery.blockUI.min.js"></script> <!-- simulate synchronous behavior when using AJAX -->
    <script src="~/Areas/Panel/assets/global/plugins/bootbox/bootbox.min.js"></script> <!-- Modal with Validation -->
    <script src="~/Areas/Panel/assets/global/plugins/mcustom-scrollbar/jquery.mCustomScrollbar.concat.min.js"></script> <!-- Custom Scrollbar sidebar -->
    <script src="~/Areas/Panel/assets/global/plugins/bootstrap-dropdown/bootstrap-hover-dropdown.min.js"></script> <!-- Show Dropdown on Mouseover -->
    <script src="~/Areas/Panel/assets/global/plugins/charts-sparkline/sparkline.min.js"></script> <!-- Charts Sparkline -->
    <script src="~/Areas/Panel/assets/global/plugins/retina/retina.min.js"></script> <!-- Retina Display -->
    <script src="~/Areas/Panel/assets/global/plugins/select2/dist/js/select2.full.min.js"></script> <!-- Select Inputs -->
    <script src="~/Areas/Panel/assets/global/plugins/icheck/icheck.min.js"></script> <!-- Checkbox & Radio Inputs -->
    <script src="~/Areas/Panel/assets/global/plugins/backstretch/backstretch.min.js"></script> <!-- Background Image -->
    <script src="~/Areas/Panel/assets/global/plugins/bootstrap-progressbar/bootstrap-progressbar.min.js"></script> <!-- Animated Progress Bar -->
    <script src="~/Areas/Panel/assets/global/plugins/charts-chartjs/Chart.min.js"></script>
    <script src="~/Areas/Panel/assets/global/js/builder.js"></script> <!-- Theme Builder -->
    <script src="~/Areas/Panel/assets/global/js/sidebar_hover.js"></script> <!-- Sidebar on Hover -->
    <script src="~/Areas/Panel/assets/global/js/application.js"></script> <!-- Main Application Script -->
    <script src="~/Areas/Panel/assets/global/js/plugins.js"></script> <!-- Main Plugin Initialization Script -->
    <script src="~/Areas/Panel/assets/global/js/widgets/notes.js"></script> <!-- Notes Widget -->
    <script src="~/Areas/Panel/assets/global/js/quickview.js"></script> <!-- Chat Script -->
    <script src="~/Areas/Panel/assets/global/js/pages/search.js"></script> <!-- Search Script -->

    <!-- BEGIN INBOX PAGE SCRIPTS -->
    <script src="~/Areas/Panel/assets/global/plugins/bootstrap-context-menu/bootstrap-contextmenu.min.js"></script>
    <script src="~/Areas/Panel/assets/global/plugins/summernote/summernote.min.js"></script> <!-- Simple HTML Editor -->
    <script src="~/Areas/Panel/assets/global/plugins/quicksearch/quicksearch.min.js"></script> <!-- Search Filter -->
    <script src="~/Areas/Panel/assets/global/plugins/charts-morris/raphael.min.js"></script> <!-- Morris Charts -->
    <script src="~/Areas/Panel/assets/global/plugins/charts-morris/morris.min.js"></script> <!-- Morris Charts -->
    <script src="~/Areas/Panel/assets/global/js/pages/mailbox.js"></script>
    <!-- END INBOX PAGE SCRIPTS -->

    <script src="~/Areas/Panel/assets/admin/layout/js/layout.js"></script>
</body>
</html>
